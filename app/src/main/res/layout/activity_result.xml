<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.test.idlep.warikangtest.MainActivity">


    <Button
        android:id="@+id/btnエラー2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="エラー"
        app:layout_constraintBottom_toTopOf="@+id/btn戻る"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.848" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="釣銭"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/txt合計金額1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:text="合計金額"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/txt集金金額1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="集金金額"
        app:layout_constraintStart_toStartOf="@+id/txt合計金額1"
        app:layout_constraintTop_toBottomOf="@+id/txt合計金額1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="計算人数"
        app:layout_constraintStart_toStartOf="@+id/txt集金金額1"
        app:layout_constraintTop_toBottomOf="@+id/txt集金金額1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="36dp"
        android:text="1人支払額"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt超お金持ち" />

    <TextView
        android:id="@+id/txt超お金持ち"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="68dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="44dp"
        android:text="割勘内訳"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt釣銭"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/txt合計人数"
        app:layout_constraintTop_toBottomOf="@+id/txt合計人数" />

    <TextView
        android:id="@+id/txt合計人数"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/txt集金金額"
        app:layout_constraintTop_toBottomOf="@+id/txt集金金額" />

    <TextView
        android:id="@+id/txt集金金額"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/txt合計金額"
        app:layout_constraintTop_toBottomOf="@+id/txt合計金額" />

    <TextView
        android:id="@+id/txt合計金額"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/txt超お金持ち人数"
        app:layout_constraintTop_toBottomOf="@+id/txt超お金持ち人数" />

    <TextView
        android:id="@+id/txt超お金持ち人数"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginLeft="112dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/txt超お金持ち" />

    <Button
        android:id="@+id/btn戻る"
        android:layout_width="363dp"
        android:layout_height="48dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="16dp"
        android:text="戻る"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>